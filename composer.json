{
	"name": "samikeijonen/uuups",
	"type": "wordpress-theme",
	"description" : "A WordPress starter theme.",
	"keywords": ["wordpress"],
	"homepage": "https://github.com/samikeijonen/uuups",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Sami Keijonen",
			"email": "sami.keijonen@foxnet.fi",
			"homepage": "https://foxland.fi"
		}
	],
	"require": {
		"php": ">=5.6",
		"composer/installers": "~1.0",
		"justintadlock/hybrid-core": "5.0.x-dev"
	},
	"require-dev": {
		"samikeijonen/phpcs-composer": "dev-master"
	},
	"scripts": {
		"install-codestandards": "\"vendor/bin/phpcs\" --config-set default_standard Foxland-Default",
		"post-install-cmd": "@install-codestandards",
		"post-update-cmd" : "@install-codestandards"
	}
}
